<zk xmlns:n="native">
	<window border="none" title="Cancelar Evento"
		action="show: slideDown({duration: 300}); hide:slideUp({duration: 300})"
		closable="true" width="48%" height="70%"
		style="iconSclass=z-icon-puzzle-piece;">
		<caption iconSclass="z-icon-cogs" />


		<div class='contenido'>

			<div align="left">
				<div class='encabezado'>Datos b√°sicos</div>
				<div class='linea-two'></div>
			</div>
			<space spacing="15%" />

			<div align="center">
				<grid>

					<rows class="">

						<row class="col-md-12">
							<div>
								Motivo
								<textbox type="text" value="" cols="4" rows="2"
									width="80%" constraint="no empty" />
							</div>
						</row>
					</rows>
				</grid>
				<separator spacing="15%" />

				<div>
					<button iconSclass="z-icon-floppy-o"
						class='boton-guardar' label="Registrar" />
					<space spacing="10%" />
					<button iconSclass="z-icon-times"
						class='boton-cancelar' label="Cancelar" />
				</div>
				<separator spacing="5%" />
			</div>
		</div>
	</window>
</zk>