<zk xmlns:n="native">
	<window border="none" title="Afiliados" action="show: slideDown({duration: 300}); hide:slideUp({duration: 300})"  closable="true" 
	width="60%" style="iconSclass=z-icon-puzzle-piece;">
		<caption iconSclass="home-icon z-icon-user"/>
	<hlayout spacing="0" apply="controllers.SocioController">



		<vlayout spacing="0" hflex="1">
			
				<div sclass="page-header" align="left">
					<label value="Registrar Afiliado" />
				</div>

				<div class='contenido ' align="center" width="100%">
