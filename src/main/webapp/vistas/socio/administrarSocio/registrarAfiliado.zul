										<label value="Cédula"
											style="padding-left: 0px !important; "  />
									</div>
									<div class="col-md-12"
										style="padding:0px">
										<textbox type="text"
												value="" width="80%"  constraint="no empty" />
											<button sclass="btn-link"
												iconSclass="z-icon-search blue" />
									</div>
								</div>
								<div class="col-md-10">
									<div class="col-md-10" style="padding-left: 0px !important; ">
										<label value="Nombre"
											style="padding-left: 0px; " />
									</div>
									<div class="col-md-10"
										style="padding:0px">
										<textbox type="text"
												value="" width="100%"  constraint="no empty" />
											
									</div>
								</div>
								<div class="col-md-10">
									<div class="col-md-10" style="padding-left: 0px !important; ">
										<label value="Segundo nombre"
											style="padding-left: 0px; " />
									</div>
									<div class="col-md-10"
										style="padding:0px">
										<textbox type="text"
												value="" width="100%"  constraint="no empty" />
											
									</div>
								</div>
								<div class="col-md-10">
									<div class="col-md-10" style="padding-left: 0px !important; ">
										<label value="Apellido"
											style="padding-left: 0px; " />
									</div>
									<div class="col-md-10"
										style="padding:0px">
										<textbox type="text"
												value="" width="100%"  constraint="no empty" />
											
									</div>
								</div>
								<div class="col-md-10">
									<div class="col-md-10" style="padding-left: 0px !important; ">
										<label value="Segundo Apellido"
											style="padding-left: 0px; " />
									</div>
									<div class="col-md-10"
										style="padding:0px">
										<textbox type="text"
												value="" width="100%"  constraint="no empty" />
											
									</div>
								</div>
								<div class="col-md-10">
									<div class="col-md-10" style="padding-left: 0px !important; ">
										<label value="Fecha de Nacimiento" class="col-md-12"/>
										<datebox id="DateFechaCaducidad" width="100%" cols="12" format="short " onCreate="self.value = new Date()" />
									</div>
									
								</div>
								<div class="col-md-8">
									<div>
										<label value="Sexo" class="col-md-12"/>
