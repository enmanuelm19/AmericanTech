<?page title="Dashboard - ZK Admin"?>
<!-- Load Bootstrap  -->
<?link href="../../assets/intranet/bootstrap/css/bootstrap.css" rel="stylesheet" type="text/css"?>


<!-- Google Open Sans font -->
<!-- Load fonts from local -->
<?link href="../../assets/intranet/css/fonts.css" rel="stylesheet" type="text/css"?>

<!-- Unmark if we want to use fonts from google server
<?link href='http://fonts.googleapis.com/css?family=Open+Sans:300,400' rel='stylesheet' type='text/css'?>
-->
<?link href="../../assets/intranet/less/style.css" rel="stylesheet" type="text/css"?>
<?link href="../../assets/intranet/less/index.css.dsp" rel="stylesheet" type="text/css"?>

<zk xmlns:n="native">
	<include src="../layouts/navbar.zul"/>             
	<hlayout id="main" spacing="0">
		<include src="../layouts/sidebar.zul"/>  
<vlayout spacing="0" hflex="1">
			<div sclass="breadcrumbs">
					<div sclass="breadcrumb">
							<span sclass="home-icon z-icon-user"/>
					    <a label="Socios" href="#"/>
							<label value="Registrar afiliado"/>
					</div>
 
			</div>

		<div class='contenedor'>
				
	 <div sclass="page-header">
	    <label value="Registrar Afiliados"/>
	  
	  </div>
		   <div class='contenido '   align="center"  >
   				 <div class="col-md-6 col-centered" 
   				  form="@id('fx') @load(vm.user) @save(vm.user, before='submit')
   				   @validator('demo.input.form_sample.FormValidator', retypedPassword=vm.retypedPassword, 
   				   captcha=vm.captcha, captchaInput=vm.captchaInput)">
        			<div class='rcorners6 form_' width="">
        			
        			<grid >
          
			            	<rows class="col-md-11">
			            		<row class="col-md-11 rw">
				                  	<div class="col-md-12 no_pad">
					              		 <div class="col-md-12 lbls">Socio Responsable</div>
					                     <textbox type="text" class="col-md-8 inps_s" value=""/>
					                     <button  iconSclass="z-icon-search"  class='btn-primary btn-primary col-md-3 btn_bus'/>
				                    </div>
				                </row>
			                  <row class="col-md-11 rw">
				                  	<div class="col-md-12 no_pad">
					              		 <div class="col-md-12 lbls">Cedula</div>
					                     <textbox type="text" class="col-md-5 inps_s" value=""/>
					                     <button  iconSclass="z-icon-search"  class='btn-primary btn-primary col-md-3 btn_bus'/>
				                    </div>
				                </row>
			                  
			                  <row class="col-md-11 rw">
			                  	<div class="col-md-12 no_pad">
				              		 <div class="col-md-12 lbls">Nombre</div>
				                     <textbox type="text" class="col-md-12 inps" value=""/>
			                    </div>
			                  </row>
			                  <row class="col-md-11 rw">
			                  	<div class="col-md-12 no_pad">
				              		 <div class="col-md-12 lbls">Apellido</div>
				                     <textbox type="text" class="col-md-12 inps" value=""/>
			                    </div>
			                  </row>
			                  <row class="col-md-11 rw">
			                  	<div class="col-md-12 no_pad">
				              		 <div class="col-md-12 lbls">Tipo de Afiliado</div>
				                     <listbox  width="100px" selectedItem="@bind(vm.dateFormat)"
			                            mold="select" tabindex="8">
			                            <listitem label="Esposa" value="" />
			                             <listitem label="Hijo" value="" />
			                         </listbox>
			                    </div>
			                  </row>
			                   <row class="col-md-11 rw">
			                  	<div class="col-md-12 no_pad">
				              		 <div class="col-md-12 lbls">Apellido</div>
				                     <datebox id="db0" cols="12" format="short " onCreate="self.value = new Date()" />	
			                    </div>
			                  </row>
			                  <row class="col-md-11 rw">
			                  	<div class="col-md-12 no_pad">
				              		 <div class="col-md-12 lbls">Correo Electronico</div>
				                     <textbox type="text" class="col-md-12 inps" value=""/>
			                    </div>
			                  </row>
			                  <row class="col-md-11 rw">
			                  	<div class="col-md-12 no_pad">
				              		 <div class="col-md-12 lbls">Telefono</div>
				                     <textbox type="text" class="col-md-12 inps" value=""/>
			                    </div>
			                  </row>
			                  <row class="col-md-11 rw">
			                  	<div class="col-md-12 no_pad">
				              		<div class="col-md-12 lbls">Intereses</div>
				                     <div>
				                     	<checkbox id="l1" label="Border"  />
				                     	<checkbox id="l2" label="Border"  />
				                     	<checkbox id="l3" label="Border"  />
				                     </div>
				                     <div>
				                     	<checkbox id="l4" label="Border"  />
				                     	<checkbox id="l5" label="Border"  />
				                     	<checkbox id="l6" label="Border"  />
				                     </div>
				                     <div>
				                     	<checkbox id="l7" label="Border"  />
				                     	<checkbox id="l8" label="Border"  />
				                     	<checkbox id="l9" label="Border"  />
				                     </div>
				                     
			                    </div>
			                  </row> 

			                 <row class="col-md-11 col-centered" id="botonera">
			                  	<hlayout width="">
			                  	
			                  		
			                  		<div>
			                    		<button  iconSclass="z-icon-floppy-o"  class='btn-primary boton-guardar' label="Registrar"/>
			                    	</div>
			                  		<div>
			                    		<button  iconSclass="z-icon-edit"  class='btn-primary boton-guardar' label="Modificar "/>
			                     	</div>
			                     	<div>
			                    		<button  iconSclass="z-icon-trash-o" class='btn-primary boton-guardar' label="Eliminar"/>
			                     	</div>
			                  	</hlayout>
			             		
			                 </row>
			 
			            </rows>
			        </grid>
			        </div>
   				 </div>

	</div>	
		
			
		</div>		
			
			
			
			
		</vlayout>
	</hlayout>

	
</zk>