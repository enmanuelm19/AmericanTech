   				   @validator('demo.input.form_sample.FormValidator', retypedPassword=vm.retypedPassword, 
   				   captcha=vm.captcha, captchaInput=vm.captchaInput)">
						<div class='form-pricing' width="80%"
							height="100%">

							<div class='encabezado'>Datos del Socio</div>
							<div class='linea-two'></div>
							<separator spacing="5%" />
						
						<grid width="690px" fixedLayout="true" >
  
			            	<rows class="col-md-12">
			            		
			                  <row class="col-md-11" >
                             	<div>
			                  		
									<hlayout>
										<div style="border:1px">
				                  			<image sclass="myimg" width="100px" height="100px"
	                      			    	  src="../assets/portal/img/img1.jpg" />
	                             		</div>
	                             		<div >
	                             			<div>
	                             				<hlayout>
		                             				<div>
		                             					<div>
		                             						Número de Carnet:
		                             					</div>
			                             				<div>
			                             					Cédula:
		                             					</div>
			                             				<div>
			                             					Nombre:
		                             					</div> 
		                             					<div>
			                             					Apellido:
		                             					</div>                            									                                                				
		                             				</div>
													<div>
			                             				<div>
			                             					<textbox type="text" value="" width="140px" />                             									                                                				

			                       							<button  iconSclass="z-icon-search"  class='btn-primary btn-primary'/>		                                                				
			                             				</div>
			                             				<div>
			                             					<textbox type="text" value="" width="140px" />                             									                                                				
			                       							<button  iconSclass="z-icon-search"  class='btn-primary btn-primary'/>		                                                				
			                             				</div>
			                             				<div>
			                             					<textbox type="text" value="" width="140px" />                             									                                                				
			                             				</div>
			                             				<div>
			                             					<textbox type="text" value="" width="140px" />                             									                                                				
			                             				</div>	                             							                             																		
													</div>
	                             				</hlayout>                             				                             					
	                             			</div>	
							           </div> 
									</hlayout>			                  		
								</div>
								<separator spacing="10%" />
						    </row>
						    
						
							
						
						<row class="col-md-10 col-left"> 
			           <div>
										<listbox id="box"
											multiple="true" checkmark="true">
											<listhead>
												<listheader
													label="Codigo Afiliado" />
												<listheader label="Nombre de Afiliado" />
											</listhead>
											<listitem>
												<listcell label="0001" />
												<listcell
													label="Juan Gonzalez" />
											</listitem>
											<listitem>
												<listcell label="0001-1" />
												<listcell label="Nicola Pernalete" />
											</listitem>
											<listitem>
												<listcell label="0001-2" />
												<listcell label="María Mendez" />
											</listitem>
											<listitem>
												<listcell label="0001-3" />
												<listcell label="Adda Gonzalez" />
											</listitem>
											
										</listbox>
									</div>
							</row>
 
						</rows>   

					</grid>
						
							<separator spacing="10px" />


							<div class='encabezado'>Datos de la Eventualidad</div>
							<div class='linea-two'></div>
								<separator spacing="30px" />
