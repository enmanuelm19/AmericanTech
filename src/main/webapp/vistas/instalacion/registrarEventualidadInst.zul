								<rows>

								
								<row>
								<vlayout>
								<hlayout>
								<div width="100px">
								<label value="Tipo de Eventualidad:" />
								</div>	
								<div>		
								<combobox width="auto"> 
						   		 <comboitem label="Pelea"/>
						   		 <comboitem label="Daño de Instalacion"/>
						   		 <comboitem label="Acto Inmoral"/>
						   		 
								 </combobox>
								</div>
								</hlayout>
								</vlayout>
								</row>
								
								<row>
								
								<vlayout>
								<hlayout>
								<div width="100px">
								
							
								
								<label value="Descripción:" />
											</div>	
											<div>
											<textbox rows="4" cols="50"/>
											</div>
								</hlayout>
								</vlayout>
								</row>
								<row>
            <separator spacing="8%"></separator>
			 </row>
			<row class="col-md-8 col-md-offset-4">
			<hlayout>
			                 
              	<div >
               	<button   iconSclass="z-icon-floppy-o"  class='boton-guardar' label="Guardar"/>
                </div>
                <div >
                <button  iconSclass="z-icon-times-circle" class='boton-cancelar' label="Cancelar" />
				</div>
            </hlayout>
			             		
			</row>
								
								</rows>
								</grid>
				

				

						</div>

					</div>

				</div>

