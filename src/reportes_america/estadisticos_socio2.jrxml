	<parameter name="IMAGEN_EQUIPO" class="java.io.InputStream"/>
	<queryString>
		<![CDATA[select DISTINCT (select count(p.*) from persona p join socio s on s.personaid_persona = p.id_persona where p.sexo ='F' and p.activo=true) + (select count(p.*) from persona p join afiliado a on a.personaid_persona = p.id_persona where p.sexo ='F' and p.activo=true) as Femenino,
		(select count(p.*) from persona p join socio s on s.personaid_persona = p.id_persona where p.sexo ='M' and p.activo=true) + (select count(p.*) from persona p join afiliado a on a.personaid_persona = p.id_persona where p.sexo ='M' and p.activo=true) as Masculino,

		CASE WHEN ((select count(p.*) from persona p join socio s on s.personaid_persona = p.id_persona where p.activo=true) + (select count(p.*) from persona p join afiliado a on a.personaid_persona = p.id_persona where p.activo=true) ) > 0 THEN
		(((select count(p.*) from persona p join socio s on s.personaid_persona = p.id_persona where p.sexo ='F' and p.activo=true) + (select count(p.*) from persona p join afiliado a on a.personaid_persona = p.id_persona where p.sexo ='F' and p.activo=true)) * 100) /
		((select count(p.*) from persona p join socio s on s.personaid_persona = p.id_persona where p.activo=true) + (select count(p.*) from persona p join afiliado a on a.personaid_persona = p.id_persona where p.activo=true) )
		ELSE '0' END as PorcentajeFemenino,

		CASE WHEN ((select count(p.*) from persona p join socio s on s.personaid_persona = p.id_persona where p.activo=true) + (select count(p.*) from persona p join afiliado a on a.personaid_persona = p.id_persona where p.activo=true)) > 0 THEN
		 (((select count(p.*) from persona p join socio s on s.personaid_persona = p.id_persona where p.sexo ='M' and p.activo=true) + (select count(p.*) from persona p join afiliado a on a.personaid_persona = p.id_persona where p.sexo ='M' and p.activo=true)) *100)/
		((select count(p.*) from persona p join socio s on s.personaid_persona = p.id_persona where p.activo=true) + (select count(p.*) from persona p join afiliado a on a.personaid_persona = p.id_persona where p.activo=true))
		ELSE '0' END  as PorcentajeMasculino
from persona p where p.activo=true]]>
	</queryString>
